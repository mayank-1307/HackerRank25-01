import java.util.*;
public class Main
{
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);
        int [] arr= new int [4];
        for(int i=0;i<4;i++)
        {
            arr[i]=sc.nextInt();
        }
        System.out.println("P1 (WT=0), P2 (WT="+arr[0]+"), P3 (WT="+(arr[0]+arr[1])+"), P4 (WT="+(arr[0]+arr[1]+arr[2])+")");
    }
}
